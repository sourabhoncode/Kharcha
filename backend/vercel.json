{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "env": {
        "NODE_ENV": "production"
    },
    "envPrefix": "VERCEL_",
    "functions": {
        "dist/main.js": {
            "memory": 1024,
            "maxDuration": 30,
            "runtime": "nodejs18.x"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ]
}