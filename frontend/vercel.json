{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "npm run build",
                "outputDirectory": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1",
            "headers": {
                "cache-control": "public, max-age=31536000"
            }
        },
        {
            "src": "/.*",
            "dest": "/index.html",
            "status": 200
        }
    ],
    "env": {
        "REACT_APP_BASEAPI": "@REACT_APP_BASEAPI"
    }
}